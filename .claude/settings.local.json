{
  "permissions": {
    "allow": [
      "Bash(base64:*)",
      "Bash(cat:*)",
      "Bash(gh pr:*)",
      "Bash(gh run view:*)",
      "Bash(git log:*)",
      "Bash(grep:*)",
      "Bash(npm install:*)",
      "Bash(npm run lint:fix:*)",
      "Bash(npm run test:prepush:*)",
      "Bash(npm test:*)",
      "WebFetch(domain:docs.expo.dev)",
      "WebFetch(domain:github.com)",
      "WebSearch"
    ],
    "deny": ["Bash(git push:*)"],
    "ask": ["Bash(gh api:*)", "Bash(chmod:*)"]
  }
}
