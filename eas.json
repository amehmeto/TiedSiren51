{
  "cli": {
    "version": ">= 9.1.0"
  },
  "build": {
    "development": {
      "node": "18.18.2",
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NODE_ENV": "development",
        "EXPO_ROUTER_APP_ROOT": "./app"
      }
    },
    "development-simulator": {
      "node": "18.18.2",
      "env": {
        "NODE_ENV": "development"
      },
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "node": "18.18.2",
      "env": {
       "NODE_ENV": "production",
        "EXPO_ROUTER_APP_ROOT": "./app"
      },
      "developmentClient": false,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "release": {
      "node": "18.18.2",
      "distribution": "store"
    },
    "production": {
      "node": "18.18.2",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}

