{
  "type": "module",
  "name": "tiedsiren51",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow npm",
    "postinstall": "npx prisma generate",
    "install:clean": "rm -rf node_modules && npm ci",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "electron:dev": "concurrently \"BROWSER=none npm run web -- --port 19006\" \"wait-on http://localhost:19006 && npm run electron:start\"",
    "lint": "npm run lint:types && npm run lint:js -- \"**/*.{js,ts,jsx,tsx}\" \".claude/**/*.json\" \"tsconfig*.json\" && npm run lint:format -- \"**/*.{js,ts,jsx,tsx,json,yml,yaml}\" && npm run lint:sh -- scripts/*.sh .husky/scripts/*.sh .claude/hooks/*.sh && npm run lint:workflow && npm run lint:schema",
    "lint:fix": "npm run lint:types && npm run lint:js:fix -- \"**/*.{js,ts,jsx,tsx}\" \".claude/**/*.json\" \"tsconfig*.json\" && npm run lint:format:fix -- \"**/*.{js,ts,jsx,tsx,json,yml,yaml}\" && npm run lint:sh -- scripts/*.sh .husky/scripts/*.sh .claude/hooks/*.sh && npm run lint:workflow && npm run lint:schema",
    "lint:types": "tsc --noEmit",
    "lint:js": "eslint",
    "lint:js:fix": "eslint --fix",
    "lint:format": "prettier --check --ignore-path .prettierignore",
    "lint:format:fix": "prettier --write --ignore-path .prettierignore",
    "lint:md": "remark --frail",
    "lint:ticket": "remark --frail .github/ISSUE_TEMPLATE/",
    "lint:ticket:fix": "remark --output --rc-path .remarkrc.fix.mjs .github/ISSUE_TEMPLATE/",
    "lint:sh": "shellcheck",
    "lint:workflow": "bash scripts/lint-workflow-scripts.sh",
    "lint:schema": "npx prisma validate",
    "test": "vitest",
    "test:prepush": "vitest --run",
    "test:cov": "vitest --run --coverage",
    "test:cov:track": "vitest --run --coverage --coverage.reporter=json && node scripts/track-coverage.js",
    "test:cov:history": "node scripts/view-coverage-history.js",
    "test:e2e": "EXPO_PUBLIC_E2E=true maestro test maestro/",
    "prebuild": "npx expo prebuild",
    "prebuild:android": "npx expo prebuild --platform android",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "build": "eas build --platform all",
    "build:local": "eas build --platform all --local",
    "build:android": "eas build --platform android",
    "build:android:local": "eas build --platform android --local",
    "build:android:apk": "eas build -p android --profile preview --no-wait",
    "build:android:apk:local": "eas build -p android --profile preview --local",
    "build:android:e2e": "EXPO_PUBLIC_E2E=true npx expo run:android",
    "build:ios": "eas build --platform ios",
    "build:ios:simulator": "eas build --profile development-simulator --platform ios",
    "prepare": "is-ci || husky",
    "check:uncommitted": "git diff-index --quiet HEAD || exit 1",
    "expo:lint": "expo lint",
    "generate:schema": "npx prisma generate",
    "graph": "node scripts/generate-dependency-graph.mjs",
    "graph:view": "npm run graph && npx --yes @mermaid-js/mermaid-cli -i docs/TICKET-DEPENDENCY-GRAPH.md -o /tmp/graph.svg && open /tmp/graph-1.svg"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "npm run lint:js:fix --",
      "npm run lint:format:fix --"
    ],
    "*.json": [
      "npm run lint:format:fix --"
    ],
    "*.md": [
      "npm run lint:md --"
    ],
    ".claude/**/*.json": [
      "npm run lint:js:fix --"
    ],
    "*.sh": [
      "npm run lint:sh --"
    ],
    "*.{yml,yaml}": [
      "npm run lint:format:fix --",
      "npm run lint:workflow --"
    ],
    "schema.prisma": [
      "npm run lint:schema"
    ]
  },
  "dependencies": {
    "@amehmeto/expo-accessibility-service": "github:amehmeto/expo-accessibility-service",
    "@amehmeto/expo-foreground-service": "github:amehmeto/expo-foreground-service",
    "@amehmeto/expo-list-installed-apps": "github:amehmeto/expo-list-installed-apps",
    "@amehmeto/tied-siren-blocking-overlay": "github:amehmeto/tied-siren-blocking-overlay",
    "@expo/vector-icons": "14.0.3",
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "5.22.0",
    "@prisma/react-native": "^6.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-google-signin/google-signin": "10.1.1",
    "@react-native-picker/picker": "2.7.5",
    "@reduxjs/toolkit": "^2.2.5",
    "@sentry/react-native": "~5.24.3",
    "@types/react-redux": "^7.1.33",
    "date-fns": "^3.6.0",
    "electron": "^30.0.3",
    "env-var": "^7.5.0",
    "eslint-config-lint": "^1.0.1",
    "events": "^3.3.0",
    "expo": "~51.0.39",
    "expo-apple-authentication": "~6.4.2",
    "expo-background-fetch": "^12.0.1",
    "expo-blur": "~13.0.3",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.10",
    "expo-linear-gradient": "^13.0.2",
    "expo-linking": "~6.3.1",
    "expo-navigation-bar": "~3.0.4",
    "expo-notifications": "~0.28.19",
    "expo-router": "~3.5.24",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-task-manager": "^11.8.2",
    "expo-web-browser": "~13.0.3",
    "firebase": "^11.9.1",
    "formik": "^2.4.5",
    "is-ci": "^4.1.0",
    "pouchdb": "^8.0.1",
    "prisma": "^5.22.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-pager-view": "6.3.0",
    "react-native-popup-menu": "^0.16.1",
    "react-native-quick-base64": "2.1.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-tab-view": "^3.5.2",
    "react-native-uuid": "^2.0.2",
    "react-native-web": "~0.19.10",
    "react-redux": "^9.1.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/pouchdb": "^6.4.2",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "@vitest/coverage-v8": "^1.6.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-local-rules": "file:eslint-plugin-local-rules",
    "eslint-plugin-no-switch-statements": "^1.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.1",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.0",
    "react-test-renderer": "18.2.0",
    "remark-cli": "^12.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-lint": "^10.0.1",
    "remark-lint-heading-style": "^4.0.1",
    "remark-lint-list-item-indent": "^4.0.1",
    "remark-lint-no-duplicate-headings-in-section": "^4.0.1",
    "remark-lint-no-undefined-references": "^5.0.2",
    "remark-validate-links": "^13.1.0",
    "semantic-release": "^22.0.12",
    "shellcheck": "^3.1.0",
    "type-fest": "^4.41.0",
    "typescript": "~5.3.3",
    "unist-util-visit": "^5.0.0",
    "vite": "^5.2.12",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "vitest-tsconfig-paths": "^3.4.1",
    "wait-on": "^7.2.0"
  },
  "private": true
}
