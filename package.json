{
  "type": "module",
  "name": "tiedsiren51",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "install:clean": "rm -rf node_modules && yarn install --frozen-lockfile",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "electron:dev": "concurrently \"BROWSER=none yarn web --port 19006\" \"wait-on http://localhost:19006 && yarn electron:start\"",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts,tsx}\" && prettier \"**/*.{js,ts,tsx}\"",
    "lint:fix": "tsc --noEmit && eslint --fix \"**/*.{js,ts,tsx}\" && prettier --write \"**/*.{js,ts,tsx}\"",
    "test": "vitest",
    "test:prepush": "vitest --run",
    "test:cov": "c8 vitest --run",
    "test:e2e": "maestro test maestro/",
    "prebuild": "npx expo prebuild",
    "prebuild:android": "npx expo prebuild --platform android",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "build": "eas build --platform all",
    "build:local": "eas build --platform all --local",
    "build:android": "eas build --platform android",
    "build:android:local": "eas build --platform android --local",
    "build:android:apk": "eas build -p android --profile preview --no-wait",
    "build:android:apk:local": "eas build -p android --profile preview --local",
    "build:ios": "eas build --platform ios",
    "build:ios:simulator": "eas build --profile development-simulator --platform ios",
    "prepare": "husky",
    "check:uncommitted": "git diff-index --quiet HEAD || exit 1",
    "expo:lint": "expo lint",
    "generate:schema": "npx prisma generate"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "yarn lint:fix"
  },
  "dependencies": {
    "@amehmeto/expo-list-installed-apps": "https://github.com/amehmeto/expo-list-installed-apps.git",
    "@expo/vector-icons": "14.0.3",
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "5.22.0",
    "@prisma/react-native": "^6.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@reduxjs/toolkit": "^2.2.5",
    "@types/react-redux": "^7.1.33",
    "date-fns": "^3.6.0",
    "electron": "^30.0.3",
    "events": "^3.3.0",
    "expo": "~51.0.39",
    "expo-background-fetch": "^12.0.1",
    "expo-blur": "~13.0.3",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.10",
    "expo-linear-gradient": "^13.0.2",
    "expo-linking": "~6.3.1",
    "expo-navigation-bar": "~3.0.4",
    "expo-notifications": "~0.28.19",
    "expo-router": "~3.5.24",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-task-manager": "^11.8.2",
    "expo-web-browser": "~13.0.3",
    "formik": "^2.4.5",
    "pouchdb": "^8.0.1",
    "prisma": "^5.22.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-pager-view": "6.3.0",
    "react-native-popup-menu": "^0.16.1",
    "react-native-quick-base64": "2.1.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-tab-view": "^3.5.2",
    "react-native-uuid": "^2.0.2",
    "react-native-web": "~0.19.10",
    "react-redux": "^9.1.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/pouchdb": "^6.4.2",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "c8": "^9.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-switch-statements": "^1.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.0",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3",
    "vite": "^5.2.12",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "vitest-tsconfig-paths": "^3.4.1",
    "wait-on": "^7.2.0"
  },
  "private": true
}
