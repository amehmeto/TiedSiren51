{
  "type": "module",
  "name": "tiedsiren51",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "engines": {
    "npm": ">=10",
    "node": ">=22"
  },
  "scripts": {
    "preinstall": "npx only-allow npm",
    "install:clean": "rm -rf node_modules && npm ci",
    "start": "expo start",
    "android": "npx expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "electron:dev": "concurrently \"BROWSER=none npm run web -- --port 19006\" \"wait-on http://localhost:19006 && npm run electron:start\"",
    "lint": "bash scripts/lint.sh",
    "lint:fix": "bash scripts/lint.sh --fix",
    "lint:types": "tsc --noEmit",
    "lint:js": "eslint",
    "lint:js:fix": "eslint --fix",
    "lint:format": "prettier --check --ignore-path .prettierignore",
    "lint:format:fix": "prettier --write --ignore-path .prettierignore",
    "lint:md": "remark --frail",
    "lint:ticket": "remark --frail .github/ISSUE_TEMPLATE/",
    "lint:ticket:fix": "remark --output --rc-path .remarkrc.fix.mjs .github/ISSUE_TEMPLATE/",
    "lint:sh": "bash scripts/lint-sh.sh",
    "lint:workflow": "bash scripts/lint-workflow-scripts.sh",
    "test": "vitest",
    "test:prepush": "vitest --run",
    "test:affected": "vitest --run --changed",
    "test:cov": "vitest --run --coverage",
    "test:cov:track": "vitest --run --coverage --coverage.reporter=json && node scripts/track-coverage.js",
    "test:cov:history": "node scripts/view-coverage-history.js",
    "test:e2e": "EXPO_PUBLIC_E2E=true maestro test maestro/",
    "prebuild": "npx expo prebuild",
    "prebuild:android": "npx expo prebuild --platform android",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "build": "eas build --platform all",
    "build:local": "eas build --platform all --local",
    "build:android": "eas build --platform android",
    "build:android:local": "eas build --platform android --local",
    "build:android:apk": "eas build -p android --profile preview --no-wait",
    "build:android:apk:local": "eas build -p android --profile preview --local",
    "build:android:e2e": "EXPO_PUBLIC_E2E=true npx expo run:android",
    "build:ios": "eas build --platform ios",
    "build:ios:simulator": "eas build --profile development-simulator --platform ios",
    "prepare": "is-ci || husky",
    "check:uncommitted": "git diff-index --quiet HEAD || exit 1",
    "expo:lint": "expo lint",
    "graph": "node scripts/ticket-graph/generate-dependency-graph.mjs",
    "graph:view": "node scripts/ticket-graph/generate-dependency-graph.mjs --ascii",
    "graph:live": "node scripts/ticket-graph/generate-dependency-graph.mjs --live"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "npm run lint:js:fix --",
      "npm run lint:format:fix --"
    ],
    "*.json": [
      "npm run lint:format:fix --"
    ],
    "*.md": [
      "npm run lint:md --"
    ],
    ".claude/**/*.json": [
      "npm run lint:js:fix --"
    ],
    "*.sh": [
      "npm run lint:sh --"
    ],
    "*.{yml,yaml}": [
      "npm run lint:format:fix --",
      "npm run lint:workflow --"
    ]
  },
  "dependencies": {
    "@amehmeto/expo-accessibility-service": "github:amehmeto/expo-accessibility-service#feat/expo-sdk-55",
    "@amehmeto/expo-foreground-service": "github:amehmeto/expo-foreground-service#feat/expo-sdk-55",
    "@amehmeto/expo-list-installed-apps": "github:amehmeto/expo-list-installed-apps#feat/expo-sdk-55",
    "@amehmeto/tied-siren-blocking-overlay": "github:amehmeto/tied-siren-blocking-overlay#feat/expo-sdk-55",
