{
  "type": "module",
  "name": "tiedsiren51",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "install:clean": "rm -rf node_modules && yarn install --frozen-lockfile",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "electron:dev": "concurrently \"BROWSER=none yarn web --port 19006\" \"wait-on http://localhost:19006 && yarn electron:start\"",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts,tsx}\" && prettier \"**/*.{js,ts,tsx}\"",
    "lint:fix": "tsc --noEmit && eslint --fix \"**/*.{js,ts,tsx}\" && prettier --write \"**/*.{js,ts,tsx}\"",
    "test": "vitest",
    "test:prepush": "vitest --run",
    "test:cov": "c8 --reporter=text --reporter=json-summary vitest --run",
    "test:cov:track": "c8 --reporter=json vitest --run && node scripts/track-coverage.js",
    "test:cov:history": "node scripts/view-coverage-history.js",
    "test:e2e": "maestro test maestro/",
    "prebuild": "npx expo prebuild",
    "prebuild:android": "npx expo prebuild --platform android",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "build": "eas build --platform all",
    "build:local": "eas build --platform all --local",
    "build:android": "eas build --platform android",
    "build:android:local": "eas build --platform android --local",
    "build:android:apk": "eas build -p android --profile preview --no-wait",
    "build:android:apk:local": "eas build -p android --profile preview --local",
    "build:ios": "eas build --platform ios",
    "build:ios:simulator": "eas build --profile development-simulator --platform ios",
    "prepare": "husky",
    "check:uncommitted": "git diff-index --quiet HEAD || exit 1",
    "expo:lint": "expo lint",
    "generate:schema": "npx prisma generate"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "yarn lint:fix"
  },
  "dependencies": {
    "@amehmeto/expo-list-installed-apps": "https://github.com/amehmeto/expo-list-installed-apps.git#feature/filter-user-and-system-apps",
    "@expo/vector-icons": "^15.0.2",
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "5.22.0",
    "@prisma/react-native": "^6.0.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-firebase/app": "^23.4.1",
    "@react-native-firebase/auth": "^23.4.1",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@reduxjs/toolkit": "^2.2.5",
    "@types/react-redux": "^7.1.33",
    "date-fns": "^3.6.0",
    "electron": "^30.0.3",
    "env-var": "^7.5.0",
    "events": "^3.3.0",
    "expo": "^54.0.13",
    "expo-apple-authentication": "~8.0.7",
    "expo-background-fetch": "~14.0.7",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.9",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-navigation-bar": "~5.0.8",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.12",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-task-manager": "~14.0.7",
    "expo-web-browser": "~15.0.8",
    "firebase": "^11.9.1",
    "formik": "^2.4.5",
    "pouchdb": "^8.0.1",
    "prisma": "^5.22.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-pager-view": "6.9.1",
    "react-native-popup-menu": "^0.16.1",
    "react-native-quick-base64": "2.2.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-uuid": "^2.0.2",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "react-redux": "^9.1.2",
    "zod": "^3.23.8"
